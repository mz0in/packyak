{
  "files.exclude": {
    "**/__pycache__": true,
    ".pycache": true,
    ".ruff_cache": true
  },
  "cSpell.words": [
    "aioboto",
    "aiobotocore",
    "aiofiles",
    "boto",
    "botocore",
    "certificatemanager",
    "dagster",
    "dedupe",
    "Fargate",
    "HDFS",
    "hudi",
    "JSII",
    "Lakehouse",
    "Lakehouses",
    "Milvus",
    "MWAA",
    "OLAP",
    "OLTP",
    "packyak",
    "pydantic",
    "Qdrant",
    "selectbox",
    "streamlit",
    "transactionally",
    "Weaviate"
  ],
  "[python]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "charliermarsh.ruff"
  },
  "python.languageServer": "Default",
  "python.analysis.typeCheckingMode": "strict",
  "python.analysis.autoImportCompletions": true,
  "python.pyright.enabled": true,
  "python.pyright.autoImportCompletions": true,
  "python.pyright.typeCheckingMode": "strict"
}
