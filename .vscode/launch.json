{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Streamlit AWS CDK",
      "type": "python",
      "request": "launch",
      "console": "integratedTerminal",
      "justMyCode": true,
      "python": "${workspaceFolder}/.venv/bin/python",
      "module": "app",
      "cwd": "${workspaceFolder}/examples/streamlit-aws-cdk"
    },
    {
      "name": "Python: packyak.synth",
      "type": "python",
      "request": "launch",
      "console": "integratedTerminal",
      "justMyCode": true,
      "python": "${workspaceFolder}/.venv/bin/python",
      "module": "packyak.synth",
      "cwd": "${workspaceFolder}/examples/streamlit-aws-cdk",
      "args": ["--root=app"],
      "env": {
        "AWS_DEFAULT_REGION": "us-east-1"
      }
    },
    {
      "name": "Python: CodeGen",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/codegen.py",
      "console": "integratedTerminal",
      "justMyCode": true,
      "python": "${workspaceFolder}/.venv/bin/python"
    }
  ]
}
